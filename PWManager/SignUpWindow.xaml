<Window x:Class="PWManager.SignUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PWManager"
        mc:Ignorable="d"
        Title="SignUpWindow" Height="350" Width="600"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="257,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148" RenderTransformOrigin="0.575,0.559"/>
        <Label Content="Sign up" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" FontSize="20"/>
        <PasswordBox HorizontalAlignment="Left" Margin="257,162,0,0" VerticalAlignment="Top" Width="148"/>
        <PasswordBox HorizontalAlignment="Left" Margin="257,207,0,0" VerticalAlignment="Top" Width="148"/>
        <Label Content="Login" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="206,115,0,0"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="186,158,0,0" VerticalAlignment="Top"/>
        <Label Content="Confirm password" HorizontalAlignment="Left" Margin="140,203,0,0" VerticalAlignment="Top"/>
        <Button Content="Sign up" HorizontalAlignment="Center" Margin="0,274,0,0" VerticalAlignment="Top" Width="62"/>
    </Grid>
</Window>
